@{
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["DefaultText"]</h3>

<table class="table table-bordered">
<tr>
    <th>Name</th>
    <th>Data</th>
</tr>
    @for (int i = 0; i < 5; i++)
    {
        <tr>
            <td>Row @(i+1)</td>
            <td>@(i+1)</td>
        </tr>
    }
</table>
<p>Use this area to provide additional information.</p>
